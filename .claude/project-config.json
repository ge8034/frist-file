{
  "projectName": "GuanDan2",
  "version": "0.1.0",
  "description": "基于 TypeScript + Next.js + React + Tailwind CSS 构建的 AI 应用项目，包含 RAG（检索增强生成）功能",
  "framework": "next.js",
  "language": "typescript",
  "ui": "tailwindcss",
  "ai": {
    "embeddingModel": "text-embedding-3-small",
    "chatModel": "gpt-4o-mini",
    "enableRag": true
  },
  "directoryStructure": {
    "agents": "src/agents",
    "rag": "src/rag",
    "embeddings": "src/embeddings",
    "ui": "src/ui",
    "types": "src/types",
    "utils": "src/utils",
    "tests": "tests",
    "docs": "docs"
  },
  "codingStandards": {
    "maxFunctionLines": 50,
    "functionNaming": "verb",
    "addComments": true,
    "typescriptTypes": true,
    "singleResponsibility": true
  },
  "dependencies": {
    "next": "14.2.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@ai-sdk/openai": "^1.0.0",
    "ai": "^4.0.0",
    "zod": "^3.23.8",
    "langchain": "^0.3.0",
    "@langchain/core": "^0.3.0"
  },
  "devDependencies": {
    "typescript": "^5.5.3",
    "tailwindcss": "^3.4.6",
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5"
  }
}
